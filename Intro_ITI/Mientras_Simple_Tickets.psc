Algoritmo Mientras_Simple_Tickets
	Definir ticket1,ticket2 como cadena
	ticket1<-"100";
	Escribir "Ingrese el precio de su ticket para pagar su adeudo";
	Leer ticket2
	Mientras ticket2<>ticket1
		Escribir "Numero de ticket incorrecto, ingrese otro de nuevo";
		Leer ticket2
		
	FinMientras
	Escribir "Ticket aceptado pagando adeudo...";
FinAlgoritmo
